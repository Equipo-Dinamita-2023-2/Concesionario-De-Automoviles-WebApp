{
  "version": 2,
  "builds": [
    {
      "src": "concesionario_webapp/wsgi.py",
      "use": "@vercel/static-build",
      "config": { "distDir": "concesionario_webapp" }
    },
    {
      "src": "pag-react/**",
      "use": "@vercel/static-build",
      "config": { "distDir": "pag-react/build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/concesionario_webapp/$1",
      "source": "/concesionario_webapp/(.*)"
    },
    {
      "src": "/pag-react/(.*)",
      "dest": "/pag-react/$1",
      "source": "/pag-react/(.*)"
    },
    {
      "src": "/static/(.*)",
      "dest": "/concesionario_webapp/static/$1",
      "source": "/concesionario_webapp/static/(.*)"
    }
  ]
}
