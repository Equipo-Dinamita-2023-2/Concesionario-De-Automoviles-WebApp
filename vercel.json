{
  "builds": [
    {
      "src": "concesionario_webapp/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "pag-react/build/**",
      "use": "@vercel/static"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "concesionario_webapp/wsgi.py"
    }
  ]
}
